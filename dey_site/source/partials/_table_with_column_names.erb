<% # generate table %>
<table class="ColumnTable">
<thead>
        <% # add specific column titles for invited talks page %>
        <tr >
            <th> Meeting </th> 
            <th> Location </th> 
            <th> Date </th> 
            <th> Other info </th> 
        </tr>
</thead>
<tbody> 
    <% source.each do |key, value| %>
    <tr>
        <% # add each key to table row %>
        <td> <%= key %> </td> 
        <% # add values of each key to table row %>
    <% value.each do |data| %>
        <td> <%= data %> </td> 
    <% end %>
    </tr>
<% end %>
</tbody>
</table>
